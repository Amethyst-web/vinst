msgid "Paid"
msgstr "Оплачен"

#: //wa-plugins/shipping/smart/templates/form.html:113
msgid "Payment"
msgstr "Оплата"

#: //wa-plugins/shipping/smart/templates/form.html:114
msgid "Shipping cost"
